<%=flash[:notice]%>
<% if user_signed_in? %>
<div class="new-artist">
  <div class="text-new-artist">
      <%= link_to 'ADD NEW ARTIST', new_artist_path%>
      <% else %>
      <% end %>
  </div>
</div>

<div class="index-artist column line-title">
