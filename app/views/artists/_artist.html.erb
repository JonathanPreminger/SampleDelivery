<tr id="tr-column">
  <td class="td-row"><p><%  if artist.image.attached?  %>
  <%= image_tag artist.image, class:'background-artist-image' %>
  <% else %>
  <% end %>
  </p>
  </td>
  <td class="td-row"><p><%= link_to artist.name.upcase, artist, id:"td-artist-name" %></p></td>
  <td class="td-row">
    <p><%= link_to 'Show', artist, class:'underline' %></p>
    <p><%= link_to 'Edit', edit_artist_path(artist), class:'underline' %></p>
    <p><%= link_to 'destroy', artist, method: :delete, data:{confirm:"sur?"}, class:'destroy-artist underline', remote: true %></p>
  </td>

</tr>
