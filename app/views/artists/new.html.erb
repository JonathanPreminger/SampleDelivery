<div class="container-edit">
  <div class="carreblanc">
    <div class="form">
      <%= form_for @artist do |f| %>
        <div class="row">
          <p>Name</p>
          <p>
          <%= f.text_field :name %><br>
          </p>
        </div>
        <div class="row">
          <p>Image</p>
          <p>
          <%= f.file_field :image %><br>
          </p>
        </div>
      <%= f.submit %>
    </div>
    <% end %>
  </div>
</div>
