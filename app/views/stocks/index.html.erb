
<h1 style="margin-top:60px">STOCK</h1>

<div style="margin-top:30px;">

  <div class="just-row tableau3">
    <p></p>
    <p>name</p>
    <p>quantity</p>
    <p>price</p>
  </div>

  <%@stocks.each do |s| %>
    <div class="just-row tableau3">
      <p><%= link_to "EDIT", edit_stock_path(s)%></p>
      <p><%=s.realrelease.name %></p>
      <p><%=s.price %></p>
      <p><%=s.quantity %></p>
    </div>
  <%end %>

</div>
