<div class="block">
  <div class="just-column margin-bottom">
    <% @djsets.each do |set| %>
    <div class="just-row">
      <div class="">
        <p> <%= image_tag 'arrow3.png' %></p>
      </div>

    <div class="block2 margin-bottom ">
      <div class="just-column">
      <div class="block2">
        <div class="just-row small-margin-bottom">
          <p class=''>For </p><p class='info'><%= set.artist.name.upcase%></p>

        </div>
        <div class="just-row">
          <p>Dj set number id:  </p><p class='info'><%= set.id %></p>
        </div>
        <div class="just-row">
          <p>At </p><p class='info'><%= set.club.upcase %></p>
        </div>
        <div class="just-row">
          <p>The : </p><p class='info'><%= set.start%></p>
        </div>
      </div>
        <div class="">
          <%= form_for(set) do |s| %>
          <div class="just-row small-margin-bottom">
            <p>Confirm the DjSet ?</p>
            <p><%=s.check_box :confirmdjset %></p>
          </div>
          <div class="just-row small-margin-bottom">
        
            <p><%= s.submit 'Confirm Djset', class:'nicebutton' %></p>
          </div>
          <%end%>
        </div>
      </div>
    </div>
  </div>

    <%end%>

  </div>
</div>
